@import "inc/fonts";
@import "../libs/bootstrap-stylus/bootstrap/variables";
@import "bootstrap/variables";
@import "inc/variables";
@import "bootstrap/mixins";
@import "inc/reset";


$color-marine = #043daf;
$color-blue = #136de9;
$color-azure = #3babe3;
$color-rose = #d52d51;
$color-red = #ff574d;
$color-orange =  #fd7443;
$color-yellow = #f9cb26;
$color-dark = #434343;
$color-brown = #95756a;
$color-gray = #959595;
$color-green = #93ba00;
$color-forest = #009327;

.tab-content
	.tab-pane
		height 0
		overflow hidden

		&.active
			height auto
			overflow visible


.field
	&_available
		display block
		width 100%

	&_textarea
		resize none

	&_size
		&_sm
			height 60px
			padding 15px

	&_textarea-size
		&_sm
			resize none
			height 180px
			padding 15px

	&_theme
		&_brown
			border-radius 3px
			border 2px solid $color-brown
			background-color white
			color $color-gray
			font-family "Gotham Pro"
			font-size 16px
			font-weight 400

.text
	&__h2
		margin 0
		color #3c434d
		font-family Intro
		font-size 24px
		font-weight 300
		line-height 24px
		text-transform uppercase

		@media (min-width $screen-sm)
			font-size 48px
			line-height 36px

	&__h3
		padding-left 20px
		margin 0 0 25px
		font-size 16px
		font-weight bold


	&__azure
		color $color-azure

.ul
	list-style-type none
	padding 0
	margin 0 0 0 18px
	line-height 1.2


	&__item
		position relative
		padding 0 0 0 25px
		margin-bottom 20px

		&:before
			content ""
			position absolute
			left 8px
			top 5px
			display block
			width 12px
			height 12px
			background $color-red
			border-radius 50%

.button
	display inline-block
	text-align center

	&_available
		display block
		width 100%
	&_type
		&_button
			border none

		&_link
			text-decoration none

			&:hover,
			&:focus,
			&:active
				text-decoration none

	&_theme
		&_call
			padding 10px 20px
			box-shadow 0 0 16px 2px rgba(0, 0, 0, 0.36)
			border-radius 20px
			border 3px solid  $color-yellow
			background-color #ffffff
			color $color-red
			font-family "Gotham Pro"
			font-size 12px
			font-weight 700
			line-height 21px
			text-decoration underline
			text-transform uppercase

			&:hover,
			&:focus,
			&:active
				color white
				background $color-red

		&_sun
			border-radius 3px
			box-shadow 0 4px 0 0 #b5803b
			background-color  $color-yellow
			color white
			font-family "Gotham Pro"
			font-weight 700
			text-transform uppercase

			&:hover,
			&:focus,
			&:active
				background darken($color-yellow, 15%)
				color white

		&_white
			padding 11px
			background rgba(255, 255, 255, 0.32)
			color white
			font-family Intro
			font-size 24px
			font-weight 600
			line-height 36px
			text-transform uppercase

			&:hover,
			&:focus,
			&:active
				opacity 0.8

		&_gallery
			padding 7.5px 20px
			border-radius 20px
			border 2px solid #ffffff
			background transparent
			color white
			font-family "Gotham Pro"
			font-size 12px
			font-weight 700
			line-height 21px
			text-transform uppercase

			&:hover,
			&:focus,
			&:active
				color $color-dark
				background white

		&_gallery-more
			padding 20px
			border 1px solid #e2e2e2
			background-color rgb(244, 245, 242)
			color $color-dark
			font-family Intro
			font-size 14px
			font-weight 600
			line-height 19px
			text-transform uppercase

			&:hover,
			&:focus,
			&:active
				background #e2e2e2

		&_hashtag
			color #2d2d2d
			font-family Lato
			font-size 14px
			font-weight 700
			border: 1px solid #dedede;
			padding 10px 8px

			&:hover,
			&:focus,
			&:active
				background #dedede
	&_size
		&_sm
			padding 12px
			font-size 14px
			line-height 21px

		&_md
			padding 18px 30px
			font-size 14px
			line-height 21px
			font-size 17px

.hr
	height 4px
	border none
	margin 0
	background linear-gradient(to right, $color-yellow, $color-red)


.link
	&_theme
		&_rose
			color $color-rose
			font-weight 400
			text-decoration underline

			&:hover,
			&:active,
			&:focus
				color $color-rose
				text-decoration none

		&_forest
			color $color-forest
			font-weight 400
			text-decoration underline

			&:hover,
			&:active,
			&:focus
				color $color-forest
				text-decoration none

.link-dashed
	display inline-block
	border-bottom 1px dashed

	&:hover,
	&:active,
	&:focus
		text-decoration none

.link-icon
	&:hover,
	&:active,
	&:focus
		text-decoration none

.svg
	display inline-block
	fill currentColor
	vertical-align middle

.header
	min-height 308px
	padding-bottom 20px
	background-color #98dcf1
	background-image url(../images/header/bg-sm.jpg)
	background-position center center
	background-size cover
	text-align center

	@media (min-width $screen-md)
		background-image url(../images/header/bg-lg.jpg)
		text-align left

	&__logo
		margin 20px 0 0
		max-width 100%
		height auto

		@media (min-width $screen-sm)
			margin 119px 0 0

	&__contacts
		margin-top 20px

		@media (min-width $screen-sm)
			float right
			margin-top 117px

.header-contacts
	font-family "Lato", sans-serif
	text-align center

	&__city
		font-size 14px
		font-weight 400
		line-height 19px
		line-height 1
		vertical-align bottom

		&-name,
		&-icon
			color $color-blue

			&:hover,
			&:active,
			&:focus
				color darken($color-blue, 20%)

	&__phone
		margin-bottom 10px
		color $color-dark
		line-height 41px

		&-code
			font-size 20px
			vertical-align top

		&-main
			font-size 31px

.nav
	background-color $color-yellow
	background-image linear-gradient(to right, $color-yellow 0%, $color-red 100%)

	&__burger
		width 40px
		height 30px
		position relative
		margin 10px 0
		transform rotate(0deg)
		transition .2s ease-in-out
		cursor pointer
		background none

		@media (min-width $screen-sm)
			display none

		span
			display block
			position absolute
			height 6px
			width 100%
			background white
			border-radius 9px
			opacity 1
			left 0
			transform rotate(0deg)
			transition .25s ease-in-out

			&:nth-child(1)
				top 0

			&:nth-child(2)
				top 12px

			&:nth-child(3)
				top 24px

		&_open
			span
				&:nth-child(1)
					top 12px
					transform rotate(135deg)

				&:nth-child(2)
					opacity 0
					left -40px

				&:nth-child(3)
					top 12px
					transform rotate(-135deg)

	&__list
		display none
		padding 10px 0
		margin 0
		list-style-type none

		&_open
			display block

		@media (min-width $screen-sm)
			display flex
			justify-content space-between
			padding 25px 10px 20px

	&__item
		padding 2px 0
		color #ffffff
		font-family "Gotham Pro"
		font-size 18px
		font-weight 400
		line-height 21px
		text-transform uppercase

		@media (min-width $screen-sm)
			padding 0
			font-size 14px
			line-height 21px
			text-transform none

		@media (min-width $screen-md)
			font-size 18px
			text-transform uppercase

.main-banner
	background-color #e9e8e8

	&__item
		position relative
		font-family Intro
		text-transform uppercase

		@media (min-width $screen-sm)
			min-height 547px
			padding-bottom 20px

	&__bg
		@media (max-width $screen-sm - 1px)
			background none !important

		@media (min-width $screen-sm)
			position absolute
			right 0
			top 0
			bottom 0
			width 79%
			background-size cover
			background-position center center

	&__text
		padding 40px 0 50px
		background url(../images/banner/pattern-xs.jpg)
		background-size 100% 100%

		@media (min-width $screen-sm)
			position absolute
			top 0
			left 0
			bottom 0
			display flex
			padding 40px 125px 95px 28px
			flex-direction column
			justify-content flex-end
			align-items flex-start
			width 65%
			background url(../images/banner/pattern.png)

		@media (min-width $screen-md)
			width 70%
		@media (min-width $screen-lg)
			width 56.6%


	&__title
		margin 0 0 7px
		color #3c434d
		font-size 20px
		font-weight 300

		@media (min-width $screen-md)
			font-size 30px

	&__slogan
		margin-bottom 25px
		color #fe6847
		font-size 25px
		font-weight 700

		@media (min-width $screen-sm)
			font-size 40px

		@media (min-width $screen-md)
			font-size 60px

	&__button
		width auto

	.slick
		&-dots
			position absolute
			bottom 0
			padding 0 0 15px
			margin 0
			list-style-type none
			text-align left

			@media (min-width $screen-sm)
				padding-left 30px

			li
				display inline-block

			li button
				width 18px
				height 18px
				padding 0
				margin-right 15px
				font-size 0
				line-height 0
				border 3px solid $color-red
				border-radius 50%
				background transparent

			.slick-active button
				background $color-rose

.project-slider
	&__item
		padding 0 15px

.project-grid
	font-size 0

	@supports(flex-wrap wrap)
		display flex
		flex-wrap wrap

	&__item
		display inline-block
		vertical-align top
		width 100%
		padding 0 15px

		margin 0 -3px

		@supports(flex-wrap wrap)
			margin 0

		@media (min-width 550px)
			width 50%

		@media (min-width $screen-sm)
			width 33.333%

		@media (min-width $screen-md)
			width 25%


.popular-project
	padding-bottom 37px
	background url(../images/pattern-1.jpg)
	text-align center

	&__title
		margin 55px 0 40px

	&__tabs
		margin-bottom 20px

	&__button
		margin-top 30px

.popular-project-tabs
	display flex
	justify-content center
	align-items flex-end
	padding 0
	margin 0
	min-width 950px
	border-bottom 3px solid white
	list-style-type none

	&__item
		position relative
		width 32%
		background rgba(255, 255, 255, 0.7)
		background-clip padding-box
		border-left 1px solid transparent
		border-right 1px solid transparent
		border-top 0 solid $color-red
		box-sizing border-box
		color #52331f
		font-family Intro
		font-size 21px
		font-weight 700
		line-height 36px
		text-transform uppercase

		&_active,
		&:hover
			border-top-width 4px
			color #3c434d
			cursor default

		&_active
			width 36%
			font-size 30px
			font-weight 600
			background #fff

	&__link


	&__item &__link
		display block
		width 100%
		padding 9px 20px 2px
		text-decoration none

	&__item_active &__link,
	&__item:hover &__link
		padding 14px 20px 7px

		&:hover,
		&:active,
		&:focus
			text-decoration none

.project-item
	position relative
	margin-bottom 30px
	background white
	box-shadow 0 0 8px rgba(0, 0, 0, 0.2)

	&_corner
		&_top-sales:before
			content ""
			position absolute
			top 0
			left 0
			width 75px
			height 75px
			background url(../images/sprite.svg) 0 0 no-repeat

	&:hover &-overlay
		opacity 1

	&__pr
		width 100%
		height auto

	&__title
		margin 0 -10px
		padding 10px 0 12px
		color $color-dark
		font-family Intro
		font-size 22px
		font-weight 700

	&__price
		padding 7px 0 3px
		border 1px solid rgba(60, 67, 77, 0.12)
		border-left none
		border-right none
		color $color-red
		font-family "Gotham Pro"
		font-size 26px
		font-weight 700
		text-transform uppercase

	&__body
		padding 0 23px

	&__foo
		padding 5px 23px 19px

	&__chars
		padding 8px 0 3px
		color $color-dark
		font-family Intro
		font-size 14px
		line-height 20px

	&__char
		margin-bottom 2px

	&__button
		padding 10px 25px 5px

.project-item-overlay
	position absolute
	top 0
	left 0
	right 0
	bottom 0
	padding 0 23px
	background rgba(55, 56, 57, 0.95)
	opacity 0
	transition opacity 0.5s

	&__header
		padding 10px 0
		border-bottom 1px solid rgba(255, 255, 255, 0.45)

	&__body
		margin-bottom 32px

	&__foo
		padding 16px 0
		border-top 1px solid rgba(255, 255, 255, 0.45)
	&__title
		padding 10px 0 6px
		color white

	&__icon
		display block
		width 72px
		height 72px
		margin 40px auto 20px
		border 2px solid $color-yellow
		border-radius 50%
		padding 12px 0 0

	&__chars
		color white

	&__button
		padding 8px 25px 7px
		font-size 26px

.banner

	&_1
		margin 27px 0 33px

	&_2
		margin 40px 0 32px

	&__container
		padding 12px 10px
		color white

		&_theme
			&_green
				background $color-green
			&_red
				background $color-red

	&__inner
		border 3px solid #ffffff
		font-family Intro
		text-transform uppercase
		padding 10px

		@media (min-width $screen-md)
			padding 0 25px

	&__title
		padding 3px 0 0px
		margin 0
		font-size 25px
		font-weight 700

		@media (min-width $scree-sm)
			font-size 45px

		@media (min-width $screen-md)
			font-size 45px

		@media(min-width $screen-lg)
			font-size 60px

	&__slogan
		margin 0 0 4px
		font-size 20px
		font-weight 400

		@media (min-width $scree-sm)
			font-size 30px
		@media(min-width $screen-lg)
			font-size 36px
	&__button
		width 245px
		padding-left 20px
		padding-right 20px

	&__grid
		@media (min-width $screen-md)
			display flex
			align-items center
			justify-content space-between

.aq
	padding 110px 0 71px
	background url(../images/pattern-2.jpg) center center

	&__quest
		margin-top 30px

		@media (min-width $screen-sm)
			margin 0

.aq-author
	position relative
	padding 50px 10px 10px
	border-radius 5px
	background white
	margin 0
	text-align center

	@media (min-width $screen-md)
		padding 50px 30px 50px

	&__pr
		position absolute
		left 50%
		top -68px
		display block
		width 110px
		height 110px
		margin-left -55px
		border 5px solid white
		border-radius 50%
		overflow hidden
		mask-image radial-gradient(circle, white 100%, black 100%)

		img
			margin -5px

	&__name
		margin-bottom 10px
		color $color-dark
		font-family Lato
		font-size 16px
		font-weight 700

	&__position
		margin-bottom 17px
		color $color-yellow
		font-family Lato
		font-size 12px
		font-weight 400
		text-transform uppercase

	&__desc
		font-family "Gotham Pro"
		font-size 14px
		font-weight 400

		@media (min-width $screen-md)
			line-height 30px

.aq-quest
	text-align center
	font-family Intro

	&__title
		margin 0 0 10px
		color $color-dark
		font-size 24px
		font-weight 600
		text-transform uppercase

	&__slogan
		margin 0 0 22px
		color #262626
		font-size 14px
		font-weight 400
		font-style italic
		line-height 24px

.aq-form
	font-size 14px
	line-height 18px

	&__field
		margin-bottom 15px

	&__button
		margin-bottom 15px

.edge
	padding 0 0 59px
	text-align center
	background white

	&__title
		margin 70px 0 25px

	&__column
		@media (min-width 480px)
			display flex
			justify-content space-around
			align-items flex-start

		@media (min-width $screen-sm)
			flex-direction column
			align-items center
			min-height 600px

		&_last,
		&_first
			@media (min-width $screen-sm) and (max-width ($screen-lg - 1px))
				min-height auto
				display block

		&_first
			@media (min-width $screen-sm) and (max-width ($screen-lg - 1px))
				margin-bottom -100px

		&_last
			@media (min-width $screen-sm) and (max-width ($screen-lg - 1px))
				margin-top -100px

.edge-item
	width 200px
	margin 0 auto 20px
	color $color-dark
	font-family Intro
	font-size 15px
	font-weight 400
	line-height 20px
	text-transform uppercase

	@media (min-width 480px)
		margin auto

	&__icon
		margin-bottom 30px

.rating
	display inline-block
	width 125px
	height 14px
	background url(../images/star-gray.svg)

	&__content
		background url(../images/star-lime.svg)
		height 14px

.reviews
	padding 0 0 64px
	background url(../images/pattern-1.jpg)
	text-align center

	&__title
		margin 45px 0 85px

	&__container
		padding 65px 0 25px
		position relative
		background white
		box-shadow 0 5px 0 0 $color-brown

	&__prs
		width 107px
		position absolute
		top -50px
		left 50%
		margin-left -55px

		@media (min-width 400px)
			width 250px
			margin-left -125px

		@media (min-width 600px)
			width 380px
			margin-left -190px

.reviews-prs
	&__item
		height 109px
		width 79px
		padding 25px 10px

	&__pr
		display block
		width 59px
		height 59px
		margin auto
		overflow hidden
		border-radius 50%
		mask-image radial-gradient(circle, white 100%, black 100%)
		transition all 0.3s

		img
			max-width 100%
			height auto

	.slick-center &__pr
		position relative
		z-index 10
		margin -25px
		width 109px
		height 109px

	.slick
		&-arrow
			width 45px
			height 45px
			border-radius 50%
			border 3px solid $color-red
			background-color #ffffff


		&-prev,
		&-next
			position absolute
			top 50%
			margin-top -20px
			font-size 0
			line-height 0
			color transparent

			&:before
				content ""
				display block
				margin auto
				width 12px
				height 18px

		&-prev
			left -65px
			&:before
				background url(../images/sprite.svg) -75px 0 no-repeat
			&:hover:before
				background url(../images/sprite.svg) -88px 0 no-repeat

		&-next
			right -65px
			&:before
				background url(../images/sprite.svg) -75px -17px no-repeat
			&:hover:before
				background url(../images/sprite.svg) -88px -17px no-repeat

.reviews-comments
	font-family Lato
	font-size 16px
	font-weight 400
	line-height 30px

	&__name,
	&__project
		color $color-dark

	&__desc,
	&__address
		color $color-gray

	&__rating
		margin-bottom 7px

	&__desc
		position relative
		width 1030px
		max-width 100%
		padding 0 5px
		margin 0 auto 22px

		@media (min-width 500px)
			padding 0 50px

		&:after,
		&:before
			display none
			position absolute
			top 50%
			margin-top -40px
			font-family Lato
			font-size 72px
			font-weight 400

			@media (min-width 500px)
				display block

		&:before
			content "«"
			left 0

		&:after
			content "»"
			right 0

.gallery
	text-align center
	background white
	padding-bottom 65px

	&__title
		margin 57px 0 44px

	&__items
		font-size 0

		@supports(flex-wrap: wrap)
			display flex
			flex-wrap wrap



	&__item
		display inline-block
		vertical-align top
		// @media (min-width $screen-sm-min)
		width 100%

		@media (min-width 550px)
			width 50%

		@media (min-width $screen-md-min)
			width 33.3333%


.gallery-tabs
	display block
	padding 0
	margin 0 0 34px
	list-style-type none

	&__item
		display inline-block

	&__link
		display inline-block
		padding 7px 20px
		border-radius 20px
		border 2px solid transparent
		background-color #ffffff
		font-family Intro
		font-size 18px
		font-weight 600
		cursor pointer

		&_active,
		&:hover,
		&:focus,
		&:active
			border 2px solid $color-red
			color $color-red
			text-decoration none

		&_active
			cursor default

.gallery-item
	padding 0 7.5px
	font-family Lato
	margin 0 -3px 25px

	@supports(flex-wrap wrap)
		margin 0 0 25px

	&__pr
		position relative
		border 10px solid #ecedf0
		margin-bottom 10px

		img
			width 100%
			height auto

	&__name
		margin-bottom 5px
		color $color-dark
		font-size 15px
		font-weight 700

	&__address
		margin-bottom 5px
		color $color-gray
